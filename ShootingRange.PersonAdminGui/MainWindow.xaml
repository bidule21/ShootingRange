<Window x:Class="ShootingRange.PersonAdminGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:personAdminGui="clr-namespace:ShootingRange.PersonAdminGui"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <personAdminGui:PersonViewModel x:Key="PersonViewModel"/>
    </Window.Resources>
    <Grid DataContext="{StaticResource PersonViewModel}">
        <ListBox HorizontalAlignment="Left" Height="109" Margin="10,10,0,0" VerticalAlignment="Top" Width="296" ItemsSource="{Binding People}"/>

    </Grid>
</Window>
