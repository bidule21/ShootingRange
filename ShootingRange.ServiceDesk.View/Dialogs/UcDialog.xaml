<wpf:CustomWindow x:Class="ShootingRange.ServiceDesk.View.Dialogs.UcDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewModel="clr-namespace:ShootingRange.ServiceDesk.ViewModel;assembly=ShootingRange.ServiceDesk.ViewModel"
             xmlns:wpf="clr-namespace:Gui.Wpf;assembly=Gui.Wpf"
             mc:Ignorable="d" 
        Title="{Binding Title}" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterOwner" Icon="/ShootingRange.ServiceDesk.View;component/Resources/flower_48.png" d:DataContext="{d:DesignInstance Type=viewModel:UcDialogViewModel}" d:DesignWidth="413" d:DesignHeight="189" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <ContentPresenter Content="{Binding CurrentPage}"/>
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right" Margin="5">
            <Button Content="OK" Height="23" Width="80" Margin="5" IsDefault="True" Click="Button_Click"/>
            <Button Content="Cancel" Height="23" Width="80" Margin="5" IsCancel="True"/>
        </StackPanel>
    </Grid>
</wpf:CustomWindow>
